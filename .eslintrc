{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "react/jsx-uses-vars": 1,
    "linebreak-style": 0
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "blockBindings": true,
      "defaultParams": true,
      "jsx": true,
      "templateStrings": true
    }
  }
}
